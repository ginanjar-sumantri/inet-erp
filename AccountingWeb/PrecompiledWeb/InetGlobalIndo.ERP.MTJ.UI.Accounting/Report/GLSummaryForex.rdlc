  <Code>PRIVATE _amount AS DOUBLE = 0
PRIVATE _accountCode AS STRING = ""

PUBLIC FUNCTION CountAmount(BYVAL _prmSaldoHome AS STRING, BYVAL _prmBalanceHome AS STRING, BYVAL _prmAccCode AS STRING) AS STRING
	DIM _result AS DOUBLE = 0

	IF ( Me._accountCode = "" ) OR ( Me._accountCode &lt;&gt; _prmAccCode )THEN
		Me._amount = 0

		Me._accountCode = _prmAccCode

		Me._amount = _prmSaldoHome + Me._amount + _prmBalanceHome
	ELSE IF Me._accountCode = _prmAccCode THEN
		Me._amount = Me._amount + _prmBalanceHome
	End IF

	_result = Me._amount
	
	IF _result = 0
		RETURN "0"
	ELSE
		RETURN Format( _result, "#,##0.00" )
	END IF	
	
END FUNCTION</Code>
